{
    "version": 2,
    "builds": [
      {
        "src": "app/**/*.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["app/**/*", "models/**/*", "routes/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app/routes/api.ts"
      }
    ]
  }
  